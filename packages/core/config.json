{
  "logger": {
    "app": "trifid",
    "level": "debug"
  },
  "listener": {
    "port": 8080
  },
  "express": {
    "trust proxy": "loopback",
    "x-powered-by": null
  },
  "i18next": {
    "debug": true,
    "detection": {
      "caches": ["cookie"],
      "cookieDomain": "trifid"
    },
    "saveMissing": true,
    "load": "unspecific",
    "whitelist": [ "de", "fr", "it", "en" ],
    "fallbackLng": [ "de", "fr", "it", "en" ],
    "preload": [ "de", "fr", "it", "en" ],
    "backend": {
      "projectId": "051613d6-9953-4e08-949b-b460fe483dbd",
      "apiKey": "c1b8521d-b2ec-4edc-9dad-fdb3e142ad59",
      "referenceLng": "en"
    },
    "_backend": {
      "loadPath": "locales/{{lng}}/{{ns}}.json",
      "addPath": "locales/{{lng}}/{{ns}}.missing.json",
      "jsonIndent": 2
    }
  },
  "patchHeaders": {
    "static": {
      "Access-Control-Allow-Origin": "*",
      "Cache-Control": "public, max-age=120",
      "Vary": "Accept"
    }
  },
  "staticFiles": {
    "rendererFiles": {
      "path": "/",
      "folder": "renderer:public"
    }
  },
  "mediaTypeUrl": {
    "formats": {
      "jsonld": "application/ld+json",
      "nt": "application/n-triples",
      "ttl": "text/turtle",
      "xml": "application/rdf+xml"
    }
  },
  "rewrite": {
    "mediaTypes": [
      "application/html",
      "application/ld+json",
      "application/n-triples",
      "application/rdf+xml",
      "application/xhtml+xml",
      "text/html",
      "text/turtle"
    ],
    "rewriteContent": true
  },
  "handler": {
    "module": "trifid-handler-fs",
    "options": {
      "path": "node_modules/tbbt-ld"
    }
  },
  "renderer": {
    "module": "trifid-renderer-simple",
    "template": "renderer:views/index.html",
    "templateError": "renderer:views/error.html"
  }
}
