{
  "baseConfig": "trifid:config.json",
  "sparqlEndpointUrl": "http://localhost:3030/tbbt/sparql",
  "patchHeaders": {
    "remove": [
      "Fuseki-Request-ID",
      "Server"
    ]
  },
  "sparqlProxy": {
    "path": "/query",
    "options": {
/*
      "authentication": {
        "user": "user",
        "password": "password"
      },
*/
    }
  },
  "handler": {
    "module": "trifid-handler-sparql",
    "options": {
      "existsQuery": "ASK { <${iri}> ?p ?o }",
      "graphQuery": "DESCRIBE <${iri}>"
    }
  },
  "yasgui": {
    "path": "/sparql",
    "options": {
      "template": "trifid:public/yasgui/index.html"
    }
  }
}
